# extension-template configuration
# Copy required settings into ${ORADBA_PREFIX}/etc/oradba_customer.conf

# Enable/disable this extension (default: true)
export ORADBA_EXT_EXTENSION_TEMPLATE_ENABLED="true"

# Set load priority (lower loads first; default: 50)
export ORADBA_EXT_EXTENSION_TEMPLATE_PRIORITY="50"

